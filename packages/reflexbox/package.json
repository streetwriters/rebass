{
  "name": "reflexbox",
  "version": "5.0.0",
  "description": "Responsive React grid system built with Styled System, with support for Emotion and Styled Components",
  "main": "dist/index.js",
  "keywords": [
    "reflexbox",
    "flexbox",
    "grid-styled",
    "rebass",
    "rebass-grid",
    "layout",
    "emotion",
    "css",
    "style"
  ],
  "scripts": {
    "prepare": "babel src -d dist && yarn styled-components",
    "styled-components": "NODE_ENV=styled babel src -d styled-components"
  },
  "dependencies": {
    "@emotion/styled": "^11.1.0",
    "@emotion/react": "^11.10.0",
    "@rebass/bundler": "^4.0.5",
    "@styled-system/css": "^5.1.5",
    "@styled-system/should-forward-prop": "^5.1.5",
    "styled-system": "^5.1.5"
  },
  "peerDependencies": {
    "react": ">=17.0.0"
  },
  "author": "Brent Jackson",
  "license": "MIT",
  "repository": "streetwriters/rebass",
  "devDependencies": {
    "babel-plugin-transform-rename-import": "^2.3.0"
  },
  "gitHead": "f0f01843e9fb63ca69112d7e97a1451b27ee9e6c"
}
